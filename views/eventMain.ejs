<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Evento</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="/css/bootstrap.min.css" />
</head>
<body>
    <div class="container">
        <div>
            <%= event.titleOfEvent %>
        </div>
        <div>
            <%= event.descOfEvent %>
        </div>
        <div>
            <% event.images.forEach(function(event) { %>
                <img src="/uploads/<%= event.name %>" alt="" width="200px" height="200px">
                <h3><%= event.captionOfEvent %></h3>
            <% }); %>
        </div>
    </div>
</body>
</html>